{
  "name": "graphql_ppx",
  "version": "0.1.0",
  "description": "Reason workflow with Esy",
  "license": "MIT",
  "esy": {
    "build": "refmterr dune build src/bucklescript/graphql_ppx.exe",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": {
      "releasedBinaries": [
        "graphql_ppx.exe"
      ],
      "deleteFromBinaryRelease": [
        "*esy_ocaml_*",
        "*refmterr-*",
        "ocaml-*"
      ]
    }
  },
  "dependencies": {
    "@opam/dune": "1.6.0",
    "@opam/result": "*",
    "@opam/yojson": "*",
    "@opam/ocaml-migrate-parsetree": "*",
    "@opam/ppx_tools_versioned": "*",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "~4.2.1"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "*",
    "ocaml": "~4.2.1"
  },
  "peerDependencies": {
    "ocaml": " >= 4.2.1  < 4.7.0"
  }
}
